

class Solution {
    public int removeDuplicates(int[] nums) {
        int a=nums[0];
        int i=1;
        int j=1;

        while (i<nums.length){
            if(nums[i]==a){
                i++;

            }else{
                nums[j]=nums[i];
                a=nums[i];
                j++;
                i++;
            }
        }
        return j;
        
    }
}
